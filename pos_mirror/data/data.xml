<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="mirror_image_1" model="mirror.images">
            <field name="name">Burger</field>
            <field name="sequence">1</field>
            <field name="image" type="base64" file="pos_mirror/static/src/img/burger.jpg"/>
        </record>
        <record id="mirror_image_2" model="mirror.images">
            <field name="name">Pizza</field>
            <field name="sequence">1</field>
            <field name="image" type="base64" file="pos_mirror/static/src/img/pizza.jpg"/>
        </record>
        <record id="mirror_image_3" model="mirror.images">
            <field name="name">Hot Dog</field>
            <field name="sequence">1</field>
            <field name="image" type="base64" file="pos_mirror/static/src/img/subway.png"/>
        </record>

        <record model="pos.config" id="point_of_sale.pos_config_main">
            <field name="mirror_image_ids" eval="[(6, 0, [ref('pos_mirror.mirror_image_1'), ref('pos_mirror.mirror_image_2'), ref('pos_mirror.mirror_image_3')])]" />
        </record>
    </data>
</openerp>
